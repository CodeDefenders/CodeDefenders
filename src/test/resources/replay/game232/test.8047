/* no package name */

import static org.junit.Assert.*;

import org.junit.*;
public class TestIntHashMap {
	@Test(timeout = 4000)
	public void test() throws Throwable {
		IntHashMap i = new IntHashMap(13, .99f);
        Integer o = 8;
        Integer o2 = 17;
        i.put(1,o);
        i.put(2,o);
        i.remove(2);
        assertFalse(i.containsKey(2));
        assertFalse(i.contains(o2));
	}
}